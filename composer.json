{
    "name": "regur/lmvc-database-migration",
    "description": "A lightweight database migration system for LMVC-based PHP applications.",
    "type": "library",
    "keywords": [
        "php",
        "database",
        "migration",
        "lmvc",
        "framework",
        "cli",
        "library"
    ],
    "homepage": "https://github.com/regur/lmvc-database-migration",
    "license": "MIT",
    "authors": [
        {
            "name": "Ravi Majithiya",
            "email": "ravi.majithiya@regur.net",
            "homepage": "https://github.com/ravimajithiya786",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "symfony/console": "^5.4",
        "composer-plugin-api": "^2.0"
    },
    "require-dev": {
        "composer/composer": "^2.8",
        "pestphp/pest": "^1.23"
    },
    "autoload": {
        "psr-4": {
            "Regur\\LMVC\\Framework\\Database\\": "src/Database",
            "Regur\\LMVC\\Framework\\Cli\\": "src/Cli",
            "Regur\\LMVC\\Framework\\Bin\\": "bin",
            "Regur\\LMVC\\Framework\\Composer\\": "src/Composer"
        }
    },
    "bin": [
        "bin/lmvcdb"
    ],
    "extra": {
        "class": "Regur\\LMVC\\Framework\\Composer\\Installer"
    },
    "support": {
        "issues": "https://github.com/ravimajithiya786/lmvc-database-migration/issues",
        "source": "https://github.com/ravimajithiya786/lmvc-database-migration"
    }
}
